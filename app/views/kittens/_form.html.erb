<%= form_with model: kitten do |f| %>
    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>
    <div>
        <%= f.label :age %>
        <%= f.text_field :age %>
    </div>
    <div>
        <%= f.label :cuteness %>
        <%= f.select :cuteness, 1..10, prompt: '1-10' %>
    </div>
    <div>
        <%= f.label :softness %>
        <%= f.select :softness, 1..10, prompt: '1-10' %>
    </div>
    <div>
        <%= f.submit %>
    </div>
<% end %>